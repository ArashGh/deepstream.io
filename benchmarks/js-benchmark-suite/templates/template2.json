{
    "description": [
        "Several users emit a lot of data at the same time.",
        "A single subscriber will switch from connected and unconnected every minute."
    ],
    "perf-emitter": {
        "type": "parallel-users-emit",
        "users": 10,
        "event": "foo",
        "data": {
            "just": "some data"
        },
        "frequency": "1 per second"
    },
    "perf-subscriber": {
        "type": "on-and-off-subscriber",
        "event": "foo",
        "on-event": "discard",
        "frequency": "1 per second",
        "wait-at-start-ms": 0,
        "timeout-ms": 10000
    },
    "options": {
        "host": "ws://127.0.0.1:8000",
        "authParams": {
            "server": "test"
        },
        "clientOptions": {
            "maxReconnectAttempts": 10000000,
            "reconnectIntervalIncrement": 100,
            "rpcAckTimeout": 20000,
            "rpcResponseTimeout": 20000,
            "subscriptionTimeout": 20000,
            "recordReadAckTimeout": 20000,
            "recordReadTimeout": 20000,
            "recordDeleteTimeout": 20000,
            "heartbeatInterval": 10000,
            "maxReconnectInterval": 2000
        }

    }
}
